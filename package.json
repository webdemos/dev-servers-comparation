{
  "name": "dev-servers-comparation",
  "version": "1.0.0",
  "description": "Servers samples for development",
  "main": "index.js",
  "scripts": {
    "start:http-server": "npm run http-server",
    "start:lite-server": "npm run lite-server",
    "start:live-server": "npm run live-server",
    "start:browser-sync": "npm run browser-sync",
    "http-server": "http-server -o -c-l ./src -p 8082",
    "lite-server": "lite-server -c lite-bs-config.js",
    "live-server": "live-server --open=./src/ --watch=./src --port=8083",
    "browser-sync": "bash b-start.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdemos/dev-servers-comparation.git"
  },
  "keywords": [],
  "author": "",
