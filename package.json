  "bugs": {
    "url": "https://github.com/webdemos/dev-servers-comparation/issues"
  },
  "homepage": "https://github.com/webdemos/dev-servers-comparation#readme",
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "http-server": "^0.10.0",
    "lite-server": "^2.3.0",
    "live-server": "^1.2.0"
  }
}
